
#include <iostream>
#include <math.h>

using namespace std;

int main()
{
    int n;
    cin>>n;
    int originaln=n;
    int cube;
    int sum=0;
    while(n>0){
        int lastdigit=n%10;
        cube=pow(lastdigit,3);
        sum=sum+cube;
        n=n/10;
    }
    if(sum==originaln){
        cout<<"this is an armstrong no"<<endl;
    }
   
    
     return 0;
}
