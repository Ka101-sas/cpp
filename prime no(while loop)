#include <iostream>
#include <math.h>

using namespace std;

int main()
{
    int n;
    cin>>n;
    int flag=0;
    int i=2;
    while(i<n){
        if(n%i==0){
            cout<<"non-prime no"<< endl;
            flag=1;
            break;}
            i++;
        
    }
    if(flag==0){
        cout<<"prime-no"<<endl;
    }
    return 0;
}
