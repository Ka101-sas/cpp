#include <iostream>
#include <climits>
using namespace std;

#include <iostream>

using namespace std;

int search(int arr[], int size){
    int key;
    int i;
    for(i=0;i<size;i++){
        if(i==key){
            cout<<i<<" "<<arr[i];
        }
    }
    return 0;
}
int main(){
    int num[10]={4,5,2,7,9,3,1,8,4,0};
    cout<<"Enter your key";
    int key;
    cin>>key;
    search(num,10);
    return 0;
    
}
