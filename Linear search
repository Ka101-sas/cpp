
#include <iostream>
#include <climits>
using namespace std;
int search(int,int);


using namespace std;

int search(int arr[], int size){
    int i;
    int key;
    cin>>key;
    for(i=0;i<size;i++){
        if(arr[i]==key){
            cout<<i<<" "<<arr[i];
        }
    }
    return 0;
}
int main(){
    int num[10]={4,5,2,7,9,3,1,8,4,0};
    search(num,10);
    return 0;
    
}
