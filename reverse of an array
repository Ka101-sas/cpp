#include <iostream>
#include <climits>
using namespace std;
int main(){
    int n;
    
    int i;
    int j;
    cout<<"Enter your array size";
    cin>>n;
    int arr[n];
    cout<<"Enter your array";
    for(int k=0;k<n;k++){
        cin>>arr[k];
    }
    i=0;
    j=n-1;
    while(i<=j){
            int temp;
            temp=arr[i];
            arr[i]=arr[j];
            arr[j]=temp;
            i++;
            j--;
        }
        
        
    
    for(int k=0;k<n;k++){
        cout<<arr[k];
    }
    
    
}
