#include <iostream>
#include <conio.h>
using namespace std;

class Complex{
    private:
     int a,b;
    public:
     Complex(int x, int y)
     {
       a=x; b=y;
     }
     Complex(int x){
         a=x;
     }
     Complex()
     {
         a=0; b=0;
     }
};
int main(){
    Complex c1(4,5), c2(5);
    return 0;
}
